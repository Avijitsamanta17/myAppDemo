      </div>
      <div class="second" style="justify-content: center;">
        <div class="profile-card">
          <div class="avatar"><img src="data:image/png;base64," alt="profile-img"></div>
          <div>
              <div  class="name"><%= user.username %></div>
              <div class="username"><%= user.fullname %></div>
          </div> 
          <div >
              <button type="button" class="btn btn-outline-danger">
                  <a href="/login">LOG OUT</a>
              </button>
          </div>
          <div>
              <form action="/upload" method="post" enctype="multipart/form-data">
                  <input type="text" name="filecaption" placeholder="Write caption">
                  <input type="file" name="file">
                  <input type="submit">
              </form>
          </div>
          <div>
            <!-- <p class="error"> error1</p> -->
            <p>_________________</p>
          </div>
          <br>
      </div>
      </div>
    </div>
    
    <div>
        <div class="cards d-inline-flex p-2 m-3">
          <% user.posts.forEach(function(post){ %>
            <div class="card p-1 m-1" style="width: 18rem; height: 10rem">
              <img src="/images/uploads/<%= post.image %>" class="card-img-top" alt="...">
              <div class="card-body">
                <h5 class="card-title"><%= post.postText %></h5>
              </div>
            </div>
          <% }) %>
            
        </div>
    </div>
